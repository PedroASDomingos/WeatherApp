
@{
    ViewData["Title"] = "Contacts";
}
<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDM2AKYy7b8ACViW2UfdetrvfRkHeaOf0c"></script>

<section id="contact" class="contact-area section-padding">
    <div class="container">
        <h2 class="blog-heading"><i class="fa fa-mobile" aria-hidden="true"></i>&nbsp; Contacts</h2>
        <hr />
        <div class="row contact-sec">
            <!--<div class="row contact-form-design-area">-->
            <div class="col-xl-8 col-lg-8 col-md-12">
                <div class="section-title ">
                    <h2>Get in Touch</h2>
                    <div class="title-separator-outer ">
                        <div class="title-separator bg-primary style-liner"></div>
                    </div>
                </div>
                <!-- START CONTACT FORM DESIGN AREA -->
                <div class="contact-form">
                    <form id="contact-form" method="post" enctype="multipart/form-data">
                        <div class="row">
                            <div class="form-group col-md-6">
                                <input type="text" name="name" class="form-control" id="contact_name" placeholder="Name" required="required">
                            </div>
                            <div class="form-group col-md-6">
                                <input type="email" name="email" class="form-control" id="contact_email" placeholder="Email" required="required">
                            </div>
                            <div class="form-group col-md-6">
                                <input type="text" name="phone" class="form-control" id="phone" placeholder="phone">
                            </div>
                            <div class="form-group col-md-6">
                                <input type="text" name="subject" class="form-control" id="contact_subject" placeholder="subject">
                            </div>
                            <div class="form-group col-md-12">
                                <textarea rows="6" name="message" class="form-control" id="message" placeholder="Your Message" required="required"></textarea>
                            </div>
                            <div class="col-md-12 text-center Default-clr-hvr">
                                <div class="actions wow fadeInUp " data-wow-delay=".4s">
                                    <input type="submit" value="send Now" name="submit" id="submit" class="Default-clr-hvr" title="Submit Your Message!">
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <!-- / END CONTACT FORM DESIGN AREA </div> -->
            </div>
            <div class="col-xl-4 col-lg-4 col-md-12">
                <div class="section-title ">
                    <h2>Contact info</h2>
                    <div class="title-separator-outer ">
                        <div class="title-separator bg-primary style-liner"></div>
                    </div>
                </div>
                <div class="single-contact wow fadeInUp" data-wow-delay=".4s">
                    <div class="contact-icon">
                        <i class="fa fa-phone"></i>
                    </div>
                    <h4>(+351) 214 990 767</h4>
                    <p>Mon-Fri 9:00 am till 6:00 pm</p>
                </div>
                <div class="clearfix"></div>
                <div class="single-contact wow fadeInUp" data-wow-delay=".6s">
                    <div class="contact-icon">
                        <i class="fa fa-envelope"></i>
                    </div>
                    <h4>info@TravelCinel.com</h4>
                    <p>We reply with in 24 hours</p>
                </div>
                <div class="clearfix"></div>
                <div class="single-contact wow fadeInUp" data-wow-delay=".2s">
                    <div class="contact-icon">
                        <i class="fa fa-map-marker"></i>
                    </div>
                    <p>
                        Rua Jau - Alto de Santo Amaro
                        <br> Lisbon , Portugal
                    </p>
                </div>
                <div class="clearfix"></div>
                <div class="contact-socials">
                    <h4>Follow Us Social</h4>
                    <ul>
                        <li><a href=""><i class="fa fa-facebook"></i></a></li>
                        <li><a href=""><i class="fa fa-google-plus"></i></a></li>
                        <li><a href=""><i class="fa fa-twitter"></i></a></li>
                        <li><a href=""><i class="fa fa-pinterest"></i></a></li>
                        <li><a href=""><i class="fa fa-behance"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- / END CONTACT DESIGN AREA -->
<!-- Map -->
<div class="contactmap">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="map1  wow fadeInUp">
                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2948.2871743706564!2d-71.06165938447522!3d42.357718779187!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89e3708485209583%3A0x6e248f25891a1cc4!2sSchool+St%2C+Boston%2C+MA+02108%2C+USA!5e0!3m2!1sen!2sin!4v1507011599646" frameborder="0" allowfullscreen></iframe>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- //Map -->